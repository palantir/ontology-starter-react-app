{
    "name": "ontology-starter-react-app",
    "version": "0.0.0",
    "description": "Example repo for building a frontend SPA on top of Foundry APIs",
    "main": "app.js",
    "private": true,
    "scripts": {
        "dev": "webpack serve",
        "build": "NODE_ENV=production webpack",
        "lint": "run-p lint:*",
        "lint:eslint": "eslint 'src/**/*.ts*'",
        "lint:prettier": "prettier src/**/*.ts*",
        "lint:stylelint": "stylelint 'src/**/*.scss'",
        "lint-fix": "run-s lint-fix:*",
        "lint-fix:prettier": "prettier --write src",
        "lint-fix:eslint": "eslint --fix",
        "lint-fix:stylelint": "stylelint 'src/**/*.scss' --fix"
    },
    "dependencies": {
        "@ontology-starter/sdk": "file:./sdk/sdk-0.9.0.tgz",
        "querystring-es3": "^0.2.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.11.1",
        "stream-browserify": "^3.0.0",
        "tslib": "~2.5.0"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.21.5",
        "@types/react": "^18.2.6",
        "@types/react-dom": "18.2.4",
        "@typescript-eslint/eslint-plugin": "^5.60.0",
        "@typescript-eslint/parser": "^5.60.0",
        "autoprefixer": "^10.4.14",
        "babel-loader": "^9.1.2",
        "css-loader": "^6.7.3",
        "dotenv": "^16.0.3",
        "dotenv-webpack": "^8.0.1",
        "eslint": "^8.40.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "html-webpack-plugin": "^5.5.1",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.23",
        "postcss-loader": "^7.3.0",
        "prettier": "^2.8.8",
        "sass": "^1.62.1",
        "sass-loader": "^13.2.2",
        "source-map-loader": "^4.0.1",
        "style-loader": "^3.3.2",
        "stylelint": "^15.6.1",
        "stylelint-config-standard-scss": "^9.0.0",
        "stylelint-order": "^6.0.2",
        "ts-loader": "^9.4.2",
        "typescript": "~5.0.4",
        "webpack": "^5.82.1",
        "webpack-cli": "^5.1.1",
        "webpack-dev-middleware": "^6.1.0",
        "webpack-dev-server": "^4.15.0"
    },
    "engines": {
        "node": ">=16.15.0"
